        <resource-file src="GoogleService-Info.plist" />

        <icon src="platforms/ios/res/ios/icon-20.png" width="20" height="20" />
        <icon src="platforms/ios/res/ios/icon-24@2x.png" width="48" height="48" />
        <icon src="platforms/ios/res/ios/icon-27.5@2x.png" width="55" height="55" />
        <icon src="platforms/ios/res/ios/icon-44@2x.png" width="88" height="88" />
        <icon src="platforms/ios/res/ios/icon-86@2x.png" width="172" height="172" />
        <icon src="platforms/ios/res/ios/icon-98@2x.png" width="196" height="196" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        
        <icon src="platforms/ios/res/ios/icon-60@3x.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="platforms/ios/res/ios/icon-60.png" width="60" height="60" />
        <icon src="platforms/ios/res/ios/icon-60@2x.png" width="120" height="120" />
