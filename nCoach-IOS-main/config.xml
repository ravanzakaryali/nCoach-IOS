<?xml version='1.0' encoding='utf-8'?>
<widget id="org.ncoach.app" version="1.0.19" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>nCoach</name>
    <description>nCoach</description>
    <author email="info@goviva.com.tr" href="https://cordova.apache.org">
        GOVIVA
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*" />
