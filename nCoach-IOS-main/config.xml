        <icon src="platforms/ios/res/ios/icon-50@2x.png" width="100" height="100" />
        <!-- iTunes Marketing Image -->
        <icon src="platforms/ios/res/ios/icon-1024.png" width="1024" height="1024" />
    </platform>
</widget>
